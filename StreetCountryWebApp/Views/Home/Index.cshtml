@model IEnumerable<StreetCountryWebApp.Models.Street>
@{
    ViewData["Title"] = "Список улиц";
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <title>StreetCountryWebApp</title>
</head>
<body>
    <h3>Streets</h3>
    <table>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>CountryId</td>
            <td></td>
        </tr>
        @foreach (var street in Model)
        {
    <tr>
        <td>@street.Id</td>
        <td>@street.Name</td>
        <td>@street.CountryId</td>
        <td><a href="~/Home/Edit/@street.Id">Edit</a></td>
        <td><a href="~/Home/Delete/@street.Id">Delete</a></td>
    </tr>
        }
    </table>
</body>
</html>
